MODULE main
VAR
    b0 : boolean;
    b1 : boolean;
ASSIGN
    init(b0) := FALSE;
    init(b1) := FALSE;
    next(b0) := !b0;
    next(b1) := ((!b0 & b1) | (b0 & !b1));
    
DEFINE
    out := toint(b0) + 2*toint(b1);

LTLSPEC
    F (out = 3);

LTLSPEC
    G ((out = 1) -> X(out = 2));

LTLSPEC
    !F (out = 3);